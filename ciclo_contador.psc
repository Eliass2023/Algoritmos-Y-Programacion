// Contar cuantas personas entraron al salon 
Algoritmo ciclo_contador
	// Un contador es generalmente un tipo de ENTERO
	Definir cont Como Entero;
	cont <- 0; // Un contador debe tener un valor inicial antes de escribir el ciclo
	
	Mientras cont < 10 Hacer
		cont <- cont + 1;
		Escribir cont;
	FinMientras
	
FinAlgoritmo
