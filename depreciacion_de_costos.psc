Algoritmo depreciación_de_costos
	//Datos de entrada
	Definir costo Como Entero;
	Definir ValorRecu Como Entero;
	Definir VidaUtil Como Entero;
	//Datos de salida
	Definir D Como Entero;
	
	//Proceso 
	costo <- 20000;
	ValorRecu <- 2000;
	VidaUtil <- 6;
	
	D <- (costo - ValorRecu) / VidaUtil
	//Salida-Resultado
	Escribir "Depreciación: ", D;
	
FinAlgoritmo
